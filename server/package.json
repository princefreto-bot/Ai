{
  "name": "tradescalpsnip",
  "version": "1.0.0",
  "description": "TradeScalpSnip - AI Trading Analysis Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "cd .. && npm run dev",
    "build:client": "cd .. && npm install && npm run build",
    "build:server": "tsc",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "db:reset": "tsx src/scripts/resetDb.ts",
    "db:reset:prod": "node dist/scripts/resetDb.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1",
    "axios": "^1.6.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
